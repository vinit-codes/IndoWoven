{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "indowoven.com"
        }
      ],
      "destination": "https://indowoven.com/$1"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "/$1",
      "permanent": true
    }
  ]
}
